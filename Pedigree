public class Pedigree {
    
    private String nome;
    private String raca;
    private String pai;
    private String mae;
    private String avoPaterno;
    private String avoMaterno;
    
    public Pedigree(String nome, String raca, String pai, String mae, String avoPaterno, String avoMaterno) {
        this.nome = nome;
        this.raca = raca;
        this.pai = pai;
        this.mae = mae;
        this.avoPaterno = avoPaterno;
        this.avoMaterno = avoMaterno;
    }
    
    public String getNome() {
        return nome;
    }
    
    public String getRaca() {
        return raca;
    }
    
    public String getPai() {
        return pai;
    }
    
    public String getMae() {
        return mae;
    }
    
    public String getAvoPaterno() {
        return avoPaterno;
    }
    
    public String getAvoMaterno() {
        return avoMaterno;
    }
    
    public void exibirPedigree() {
        System.out.println("Nome: " + nome);
        System.out.println("Raça: " + raca);
        System.out.println("Pai: " + pai);
        System.out.println("Mãe: " + mae);
        System.out.println("Avô paterno: " + avoPaterno);
        System.out.println("Avó materna: " + avoMaterno);
    }
}
